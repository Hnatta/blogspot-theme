#clock #time {
  display: flex;justify-content: center;
}
#clock #time div {
  position: relative;
  margin: 0 1px;
  --webkit-box-reflect: below 5px linear-gradient(transparent, #0004);
  }
#clock #time div span {
  position: realtive;
  display: block;
  background: #eee;
  width: 90px;
  height: 80px;
  color: #c70101;
  display: flex;
  justify-content: center;
  align-items: center;
